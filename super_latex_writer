description = f"""

*** script executed = {scriptname} ***

This script accepts schedule grids from sunday_scheduler and produces
Latex files.

Created on Thu Dec 23 19:25:05 2021

@author: Victor Prieto

"""


# %% start runtime
start_time = time.time()
